{
  "name": "mtrCFP",
  "version": "1.0.0",
  "description": "",
  "main": "./server/server.js",
  "config": {
    "server": 3010,
    "logLevel": "debug",
    "useragent": "request (nodejs) - Newsflash/CFP (HRNM - Webmaster/TTP)"
  },
  "scripts": {
    "debug": "PORTS=${npm_package_config_server} DB=${npm_config_couchdb} node ./nodeapp/server.js",
    "debug:watch": "PORTS=${npm_package_config_server} DB=${npm_config_couchdb} nodemon --watch server ./nodeapp/server.js",
    "push": "cd sofaapp; kanso push ${npm_config_couchdb}; cd ..",
    "push:watch": "watch 'npm run push' sofaapp/",
    "watch": "npm run push:watch & npm run debug:watch",
    "build:s2": "cd sofaapp; kanso push ${npm_config_couchdb2}; cd ..;PORTS=-1 DB=${npm_config_couchdb2} node ./nodeapp/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "couchapp": "^0.11.0",
    "deep-diff": "^0.3.3",
    "eslint": "^2.7.0",
    "express": "^4.13.4",
    "moment": "^2.11.0",
    "pouchdb": "^5.3.1",
    "pouchdb-upsert": "^2.0.0",
    "request": "^2.67.0",
    "socket.io": "^1.4.5",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.0.1",
    "xpath-stream": "0.0.8"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "jshint": "^2.9.1-rc2",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "watch": "^0.17.1"
  }
